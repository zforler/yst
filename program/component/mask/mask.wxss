.mask{
    width:100%;
    height: 100%;
    background-color: rgba(0,0,0,0.5);
    position: fixed;
    top: 0px;
    left:0;
    z-index: 999;
}
.mask-process-c{
    width: 100%;
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
}
.mask-process{
    width:250px;
    padding: 15px 10px;
    border-radius: 8px;
    background-color: #fff;
}
.mask-process-text{
    text-align: center;
}